<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.oscarehr.PMmodule.model">
	<class name="SdmtIn" table="sdmt_in">
		<id column="recordid" name="recordId" type="integer" unsaved-value="0" >
			<generator class="sequence">
   			 <param name="sequence">seq_sdmt_in</param>
			</generator>
		</id>		
		<property column="batch_no" name="batchNumber"></property>
		<property column="batch_date"  name="batchDate"></property>
		<property column="first_name" name="firstName"></property>
		<property column="last_name" name="lastName"></property>
		<property column="dob" name="dob"></property>
		<property column="ben_unit_status" name="benefitUnitStatus"></property>
		<property column="program" name="program"></property>
		<property column="office" name="office"></property>
		<property column="termination_date" name="terminationDate"></property>
		<property column="sdmt_ben_unit_id" name="sdmtBenUnitId"></property>	
		<property name="lastBenMonth" column="last_ben_month"></property>
		<property column="total_payment" name="totalPayment"></property>
		<property column="basic_amount" name="basicAmount"></property>
		<property column="housing_amount" name="housingAmount"></property>		
		<property column="address" name="address"></property>
		<property column="status" name="status"></property>
		<property column="role" name="role"></property>
		<property column="paid_date" name="paidDate"></property>
		<property column="client_id" name="clientId"></property>	
	</class>
</hibernate-mapping>