<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.oscarehr.PMmodule.model">
	<class name="QuatroIntakeAnswer" table="intake_answer">
		<id name="id" type="integer" column="intake_answer_id" >
			<generator class="sequence">
	   			 <param name="sequence">seq_intake_answer_id</param>
			</generator>
		</id>

	    <property name="intakeNodeId" column="intake_node_id"/>
		<property name="value" column="val" type="string" />
		<property name="lastUpdateDate" column="lastupdatedate"></property>
		<property name="lastUpdateUser" column="lastUpdateUser"></property>
		<many-to-one name="intake2"
			class="org.oscarehr.PMmodule.model.QuatroIntakeDB" lazy="false"
			insert="false" update="false">
			<column name ="intake_id" />
		</many-to-one>
		
	</class>
</hibernate-mapping>