<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.oscarehr.PMmodule.model">
	<class name="QuatroIntakeAnswer" table="intake_answer">
        <composite-id>
          <key-property name="id" type="integer" column="intake_answer_id" />
		  <key-property name="intakeNodeId" column="intake_node_id"/>
        </composite-id>

		<property name="intakeId" column="intake_id" type="integer" />
		<property name="value" column="val" type="string" not-null="true" />

		<many-to-one name="intake"
			class="org.oscarehr.PMmodule.model.QuatroIntakeDB" column="intake_id"
			not-null="true" insert="false" update="false" />
		
	</class>
</hibernate-mapping>