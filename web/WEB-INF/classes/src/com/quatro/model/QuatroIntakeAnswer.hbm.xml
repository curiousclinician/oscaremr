<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.quatro.model">
	<class name="QuatroIntakeAnswer" table="intake_answer">
		<id name="id" type="integer" column="intake_answer_id">
			<generator class="native" />
		</id>

		<property name="intakeId" column="INTAKE_ID" />
		<property name="intakeNodeId" column="INTAKE_NODE_ID" />
		<property name="val" column="val" />
	</class>
</hibernate-mapping>