<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    author JZhang
-->
<hibernate-mapping>
    <class name="com.quatro.model.IncidentValue" table="INCIDENT" >
        <id name="id" type="java.lang.Integer">
            <column name="ID" precision="10" scale="0" />
			<generator class="sequence">
   			 <param name="sequence">seq_incident_id</param>
			</generator>
        </id>
        <property name="createdDate" type="java.util.Calendar">
            <column name="CREATED_DATE" length="7" />
        </property>
        <property name="providerNo" type="java.lang.String">
            <column name="PROVIDER_NO" length="6" />
        </property>
        <property name="incidentDate" type="java.util.Calendar">
            <column name="INCIDENT_DATE" length="7" />
        </property>
        <property name="incidentTime" type="java.lang.String">
            <column name="INCIDENT_TIME" length="8" />
        </property>
        <property name="clients" type="java.lang.String">
            <column name="CLIENTS" length="1000" />
        </property>
        <property name="staff" type="java.lang.String">
            <column name="STAFF" length="1000" />
        </property>
        <property name="witnesses" type="java.lang.String">
            <column name="WITNESSES" length="1000" />
        </property>
        <property name="otherInvolved" type="java.lang.String">
            <column name="OTHER_INVOLVED" length="100" />
        </property>
        <property name="nature" type="java.lang.String">
            <column name="NATURE" length="100" />
        </property>
        <property name="location" type="java.lang.String">
            <column name="LOCATION" length="200" />
        </property>
        <property name="clientIssues" type="java.lang.String">
            <column name="CLIENT_ISSUES" length="100" />
        </property>
        <property name="description" type="java.lang.String">
            <column name="DESCRIPTION" length="4000" />
        </property>
        <property name="disposition" type="java.lang.String">
            <column name="DISPOSITION" length="100" />
        </property>
        <property name="restriction" type="java.lang.String">
            <column name="RESTRICTION" length="1000" />
        </property>
        <property name="chargesLaid" type="java.lang.String">
            <column name="CHARGES_LAID" length="2" />
        </property>
        <property name="policeReportNo" type="java.lang.String">
            <column name="POLICE_REPORT_NO" length="20" />
        </property>
        <property name="badgeNo" type="java.lang.String">
            <column name="BADGE_NO" length="20" />
        </property>
        <property name="investigationRcmd" type="java.lang.String">
            <column name="INVESTIGATION_RCMD" length="4000" />
        </property>
        <property name="investigationConductedby" type="java.lang.String">
            <column name="INVESTIGATION_CONDUCTEDBY" length="4000" />
        </property>
        <property name="investigationDate" type="java.util.Calendar">
            <column name="INVESTIGATION_DATE" length="7" />
        </property>
        <property name="followupInfo" type="java.lang.String">
            <column name="FOLLOWUP_INFO" length="1000" />
        </property>
        <property name="followupCompletedby" type="java.lang.String">
            <column name="FOLLOWUP_COMPLETEDBY" length="200" />
        </property>
        <property name="followupDate" type="java.util.Calendar">
            <column name="FOLLOWUP_DATE" length="7" />
        </property>
        <property name="reportCompleted" type="java.lang.String">
            <column name="REPORT_COMPLETED" length="1" />
        </property>
        <property name="programId" type="java.lang.Integer">
            <column name="PROGRAM_ID" precision="10" scale="0" />
        </property>
    </class>
</hibernate-mapping>
