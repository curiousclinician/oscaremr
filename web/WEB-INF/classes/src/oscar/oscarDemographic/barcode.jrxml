<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jasperReport PUBLIC "-//JasperReports//DTD Report Design//EN" 
          "http://jasperreports.sourceforge.net/dtds/jasperreport.dtd">

<jasperReport name="BarbecueReport" pageWidth="171" pageHeight="108" columnWidth="153" leftMargin="9" rightMargin="9" topMargin="27" bottomMargin="9">
	<import value="net.sourceforge.barbecue.BarcodeFactory"/>
	<parameter name="demo" isForPrompting="true" class="java.lang.String"/>
	<title>
		<band height="72">
			<image scaleImage="Clip" hAlign="Center">
				<reportElement x="0" y="0" width="153" height="63"/>
				<graphicElement/>
				<imageExpression class="net.sf.jasperreports.engine.JRRenderable">
                 <![CDATA[new net.sf.jasperreports.renderers.BarbecueRenderer(BarcodeFactory.createCode128B($P{demo}))]]>
                </imageExpression>
			</image>
		</band>
	</title>
</jasperReport>
